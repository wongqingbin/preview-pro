{
  "name": "preview-pro",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "dev:force": "vite --force",
    "build": "vue-tsc --noEmit && vite build",
    "build:ghpages": "vue-tsc --noEmit && vite build --base=/preview-pro/ --mode=ghpages",
    "serve": "vite preview",
    "lint": "eslint --ext .ts,.tsx,.js,.vue src/ --fix",
    "lint:check": "eslint src/**/*.{ts,tsx,vue}",
    "prettier": "prettier -c --write 'src/**/*'"
  },
  "dependencies": {
    "@ant-design-vue/pro-layout": "^3.2.0-alpha.5",
    "@ant-design/icons-vue": "^6.0.1",
    "ant-design-vue": "^3.0.0-alpha.16",
    "moment": "^2.29.1",
    "vue": "^3.2.31",
    "vue-router": "^4.0.11"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vitejs/plugin-vue": "^2.2.4",
    "@vue/compiler-sfc": "^3.2.31",
    "eslint": "^8.3.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.1.1",
    "less": "^4.1.2",
    "prettier": "^2.5.0",
    "typescript": "^4.5.2",
    "unplugin-auto-import": "^0.5.3",
    "unplugin-vue-components": "^0.17.6",
    "vite": "^2.8.6",
    "vite-plugin-optimize-persist": "^0.1.1",
    "vite-plugin-package-config": "^0.1.0",
    "vue-tsc": "^0.3.0"
  },
  "author": {
    "email": "18x@loacg.com",
    "name": "Sendya",
    "url": "https://github.com/sendya"
  },
  "vite": {
    "optimizeDeps": {
      "include": [
        "@ant-design-vue/pro-layout",
        "ant-design-vue/es",
        "ant-design-vue/es/avatar/style",
        "ant-design-vue/es/breadcrumb/style",
        "ant-design-vue/es/button/style",
        "ant-design-vue/es/card/style",
        "ant-design-vue/es/col/style",
        "ant-design-vue/es/config-provider/style",
        "ant-design-vue/es/descriptions/style",
        "ant-design-vue/es/divider/style",
        "ant-design-vue/es/drawer/style",
        "ant-design-vue/es/dropdown/style",
        "ant-design-vue/es/layout/style",
        "ant-design-vue/es/menu/style",
        "ant-design-vue/es/message/style",
        "ant-design-vue/es/modal/style",
        "ant-design-vue/es/notification/style",
        "ant-design-vue/es/pagination/style",
        "ant-design-vue/es/radio/style",
        "ant-design-vue/es/result/style",
        "ant-design-vue/es/row/style",
        "ant-design-vue/es/space/style",
        "ant-design-vue/es/statistic/style",
        "ant-design-vue/es/style",
        "ant-design-vue/es/switch/style",
        "ant-design-vue/es/tag/style",
        "vue",
        "vue-router"
      ]
    }
  }
}
